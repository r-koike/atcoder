  "latex-workshop.latex.tools": [
    {
      "name": "ptex2pdf",
      "command": "ptex2pdf",
      "args": [
        "-interaction=nonstopmode",
        "-l",
        "-ot",
        "-kanji=utf8",
        "-synctex=1",
        "%DOCFILE%.tex"
      ]
    },
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": ["-interaction=nonstopmode", "-synctex=1", "%DOCFILE%.tex"]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": ["-interaction=nonstopmode", "-synctex=1", "%DOCFILE%.tex"]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"]
    },
    {
      "name": "pdflatex_wsl",
      "command": "wsl.exe",
      "args": [
        "pdflatex",
        "-interaction=nonstopmode",
        "-l",
        "-ot",
        "-kanji=utf8",
        "-synctex=1",
        "%DOCFILE%.tex"
      ]
    },
    {
      "name": "bibtex_wsl",
      "command": "wsl.exe",
      "args": ["bibtex", "%DOCFILE%"]
    },
    {
      "name": "ptex2pdf_wsl",
      "command": "wsl.exe",
      "args": [
        "ptex2pdf",
        "-interaction=nonstopmode",
        "-l",
        "-ot",
        "-kanji=utf8",
        "-synctex=-1",
        "%DOCFILE%.tex"
      ]
    },
    {
      "name": "pbibtex_wsl",
      "command": "wsl.exe",
      "args": ["pbibtex", "%DOCFILE%", "-kanji=utf8"]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "toolchain",
      "tools": ["ptex2pdf_wsl", "pbibtex_wsl"]
    }
